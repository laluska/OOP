#include <iostream>
#include <vector>
#include <string>
#include <algorithm>
#include <cstdlib>
#include <iterator>

using namespace std;

void funkcija(vector<int> v, vector<int>& v1, vector<int>& v2) {
    auto it = remove_if(v.begin(), v.end(), [&](int x) { return x < 0; });
    v.erase(it, v.end());

    transform(v.begin(), v.end(), back_inserter(v1),
        [&](int z) { return abs(z); });

    copy_if(v.begin(), v.end(), back_inserter(v2),
        [&](int j) { return j % 2 == 0; });
}

int main() {
    vector<int> v = { 1, 2, -3, -4, 5, -6 };
    vector<int> v1;
    vector<int> v2;

    funkcija(v, v1, v2);

    for (int x : v1) cout << x << " ";
    cout << endl;

    for (int x : v2) cout << x << " ";
    cout << endl;

    return 0;
}
